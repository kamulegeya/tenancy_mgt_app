<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/Azure"
     >
    
       
    <TextView android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tenant"
        android:textSize="18sp"   
        android:layout_marginLeft="15sp"
         android:lineSpacingExtra="15sp"     
        android:paddingBottom="5sp"/>
    <TextView android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         android:layout_marginLeft="15sp"
         android:text="@string/date"
          android:lineSpacingExtra="15sp" 
         android:layout_below="@+id/textView1"
        android:textSize="15sp"
        android:paddingBottom="5sp"/>
    <TextView android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         android:layout_marginLeft="15sp"
        android:textSize="15sp"
         android:lineSpacingExtra="15sp" 
         android:layout_below="@+id/textView2"
         android:text="@string/reading"
        android:paddingBottom="5sp"/>
    <TextView android:id="@+id/textView4"
         android:layout_marginLeft="15sp"
        android:layout_width="wrap_content"
         android:lineSpacingExtra="15sp" 
        android:layout_height="wrap_content"
         android:text="@string/prreadingdate"
          android:layout_below="@+id/textView3"
        android:textSize="15sp"
        android:paddingBottom="5sp"/>
    <TextView android:id="@+id/textView5"
        android:layout_width="wrap_content"
         android:layout_marginLeft="15sp"
        android:layout_height="wrap_content"
         android:lineSpacingExtra="15sp" 
        android:textSize="15sp"
         android:layout_below="@+id/textView4"
        android:text="@string/prereading"
        android:paddingBottom="5sp"/>
    <TextView android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         android:layout_marginLeft="15sp"
         android:lineSpacingExtra="15sp" 
        android:text="@string/units"
         android:layout_below="@+id/textView5"
        android:textSize="15sp"
        android:paddingBottom="5sp"/>
    <TextView android:id="@+id/textView7"
        android:layout_width="wrap_content"
         android:layout_marginLeft="15sp"
         android:lineSpacingExtra="15sp" 
        android:text="@string/rate"
        android:layout_height="wrap_content"
         android:layout_below="@+id/textView6"
        android:textSize="15sp"
        android:paddingBottom="5sp"/>    
    
    <TextView
        android:id="@+id/textView8"         
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="5sp"
        android:text="@string/amount"
        android:layout_marginLeft="15sp"
        android:layout_below="@+id/textView7"
        android:textSize="15sp" />

    <TextView  android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView1"
        android:layout_alignBottom="@+id/textView1"
        android:layout_toRightOf="@+id/textView1"
        android:text="Date Here"
        android:layout_marginLeft="30sp"    
        android:layout_alignParentRight="true"        
        />
    <TextView  android:id="@+id/textView10"
         android:textColor="@color/green" 
        android:layout_marginLeft="30sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView2"
        android:layout_alignBottom="@+id/textView2"
        android:layout_toRightOf="@+id/textView2"
        android:layout_alignParentRight="true"        
        />
<TextView  android:id="@+id/textView11"
     android:textColor="@color/green" 
    android:layout_marginLeft="30sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView3"
        android:layout_alignBottom="@+id/textView3"
        android:layout_toRightOf="@+id/textView3"
        android:layout_alignParentRight="true"        
        />
<TextView  android:id="@+id/textView12"
    android:layout_marginLeft="30sp"
     android:textColor="@color/green" 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView4"
        android:layout_alignBottom="@+id/textView4"
        android:layout_toRightOf="@+id/textView4"
        android:layout_alignParentRight="true"        
        />
<TextView  android:id="@+id/textView13"
    android:layout_marginLeft="30sp"
     android:textColor="@color/green" 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignBottom="@+id/textView5"
        android:layout_toRightOf="@+id/textView5"
        android:layout_alignParentRight="true"        
        />
<TextView  android:id="@+id/textView14"
    android:layout_marginLeft="30sp"
     android:textColor="@color/green" 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView6"
        android:layout_alignBottom="@+id/textView6"
        android:layout_toRightOf="@+id/textView6"
        android:layout_alignParentRight="true"        
        />
<TextView  android:id="@+id/textView15"
    android:layout_marginLeft="30sp"
        android:layout_width="wrap_content"
         android:textColor="@color/green" 
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView7"
        android:layout_alignBottom="@+id/textView7"
        android:layout_toRightOf="@+id/textView7"
        android:layout_alignParentRight="true"        
        />
<TextView  android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_marginLeft="30sp"
         android:textColor="@color/green" 
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView8"
        android:layout_alignBottom="@+id/textView8"
        android:layout_toRightOf="@+id/textView8"
        android:layout_alignParentRight="true"        
        />
  
</RelativeLayout>
